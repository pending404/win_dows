            filter: blur(6px);
            transition: filter 0.1s ease-in-out;
            user-select: none; /* Prevent copying of blurred text */
            padding: 20px;
            margin: 10px;
            background: #f0f0f0;
            border-radius: 10px;
        }

        /* Exclude Adobe Illustrator */
        .grid-item[data-software="Adobe Illustrator 2025"] {
            filter: none;
            user-select: text; /* Allow text selection in Adobe Illustrator */
            background-color: #ffffff; /* Solid background color */
            opacity: 1; /* Ensure the container is fully opaque */
            z-index: 10; /* Ensure it stays on top of other content */
        }

        /* Desktop View: Increase size, center, and ensure Adobe Illustrator container has solid background and normal blend mode */
        @media (min-width: 1024px) {
        .grid-item[data-software="Adobe Illustrator 2025"] {
            transform: scale(2); /* Double size on desktop */
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%) scale(2); /* Center the container */
            background-color: #ffffff; /* Solid background color, non-transparent */
            opacity: 1; /* Ensure the container is fully opaque */
            mix-blend-mode: normal; /* Disable blending effect to avoid transparency */
            z-index: 10; /* Ensure it stays on top of other content */
            }
        }
        
        /* Exclude Adobe Illustrator and add shadow */
        .grid-item[data-software="Adobe Illustrator 2025"] {
                filter: none;
                user-select: text; /* Allow text selection in Adobe Illustrator */
            background-color: #ffffff; /* Solid background color */
            opacity: 1; /* Ensure the container is fully opaque */
            z-index: 10; /* Ensure it stays on top of other content */
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2); /* Add shadow around the container */
        }

        /* Desktop View: Increase size, center, and ensure Adobe Illustrator         container has solid background and normal blend mode */
        @media (min-width: 1024px) {
        .grid-item[data-software="Adobe Illustrator 2025"] {
            transform: scale(2); /* Double size on desktop */
            position: absolute;
            top: 50%;
            left: 50%;
            transform: translate(-50%, -50%) scale(2); /* Center the container */
            background-color: #ffffff; /* Solid background color, non-transparent */
            opacity: 1; /* Ensure the container is fully opaque */
            mix-blend-mode: normal; /* Disable blending effect to avoid transparency */
            z-index: 10; /* Ensure it stays on top of other content */
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.3); /* Stronger shadow on larger screens */
    }
    
    <script>
        // Function to handle copying of text
        document.addEventListener('copy', function(e) {
            // Prevent copying of blurred sections (except Adobe Illustrator)
            const selectedText = window.getSelection().toString();
            if (selectedText && !e.target.closest('[data-software="Adobe Illustrator 2025"]')) {
            e.preventDefault(); // Prevent default copying behavior
            e.clipboardData.setData('text/plain', 'Pay Karo Password Lo'); // Set clipboard content to custom message
            }
        });
    </script>

